{
  "manifest version":1,

  "name":"Procrastireader",
  "description":"This extension allows you to save pages to read later at the click of a button",
  "version":"1.0",

  "browser_action":{
    "default_icon":{
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Procrastireader",
    "default_popup": "popup.html"
  },

  "permissions":[
    "activeTab"
  ]
}
